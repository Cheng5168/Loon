#!name = Spotify解锁
#!desc = 解锁部分premium，音质不能设置为超高，去除播放广告，歌手/专辑列表正常展示，去除随机播放。
#!date = 2025-08-31 20:00:00
#!author = app2smile[https://github.com/app2smile]
#!icon = https://raw.githubusercontent.com/Cheng5168/IconResource/main/App_icon/Spotify.png
#!loon_version = 3.3.5(904)

[Script]
http-request ^https:\/\/(spclient\.wg\.spotify\.com|.*-spclient\.spotify\.com(:443)?)\/user-customization-service\/v1\/customize$ script-path=https://raw.githubusercontent.com/Cheng5168/Loon/main/JS/Spotify/spotify-qx-header.js, timeout=60, tag=spotify-qx-header
http-response ^https:\/\/(spclient\.wg\.spotify\.com|.*-spclient\.spotify\.com(:443)?)\/(bootstrap\/v1\/bootstrap|user-customization-service\/v1\/customize)$ script-path=https://raw.githubusercontent.com/Cheng5168/Loon/main/JS/Spotify/spotify-proto.js, requires-body=true, binary-body-mode=true, timeout=60, tag=spotify-proto
http-request ^https:\/\/(spclient\.wg\.spotify\.com|.*-spclient\.spotify\.com(:443)?)\/(artistview\/v1\/artist|album-entity-view\/v2\/album)\/ script-path=https://raw.githubusercontent.com/Cheng5168/Loon/main/JS/Spotify/spotify-json.js, timeout=60, tag=spotify-json

[MITM]
hostname = spclient.wg.spotify.com, *spclient.spotify.com